
RenderState rs
{
	FrontFace						= CW;
	CULLMODE						= Back;

	DepthEnable						= TRUE;
	DepthFunc						= LESS;
	DepthWriteMask					= ALL;
	StencilEnable					= false;
	BlendEnable						= false;
};


technique dr_render_gbuffer
{
	pass p1
	{
		SetRenderState(rs);

		SetVertexShader("shader/editor_shape.vs");
		SetPixelShader("shader/editor_shape.ps");
	}
}
