

RenderState rs
{
	CULLMODE						= back;

	DepthEnable						= TRUE;
	DepthFunc						= LESS;
	DepthWriteMask					= ALL;
	StencilEnable					= false;

	BLENDENABLE						= false;

	ColorWrite						= all;
	
};
technique dr_render_gbuffer
{
	pass p1
	{
		SetRenderState(rs);
		SetVertexShader("./shader/voxel_world.vs");
		SetPixelShader("./shader/voxel_world.ps");
	}
}
