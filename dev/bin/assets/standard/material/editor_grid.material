

RenderState rs
{
	FrontFace	= CW;
	CULLMODE	= Front;
	FILLMODE	= SOLID;

	DepthEnable = TRUE;
	DepthFunc = LESS;
	DepthWriteMask = ALL;
	StencilEnable = false;
	BlendEnable = false;
};

technique grid
{
  pass p1
  {
	SetRenderState(rs);
	SetVertexShader("./shader/editor_grid.vs");
	SetPixelShader("./shader/editor_grid.ps");
  }
}
