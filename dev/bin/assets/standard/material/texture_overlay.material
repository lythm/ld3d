
RenderState rs
{
	CULLMODE						= None;
	DepthEnable						= False;
	DepthFunc						= ALWAYS;
	DepthWriteMask					= ZERO;
	StencilEnable					= false;
	BlendEnable						= true;
	BlendSrcRGB						= SRC_ALPHA;
	BlendDstRGB						= INV_SRC_ALPHA;
	BLENDRGBOP						= ADD;
	BlendSrcAlpha					= ONE;
	BlendDstAlpha					= ONE;
	BLENDALPHAop					= ADD;

};

Technique T1
{
	Pass P1
	{
		SetRenderState(rs);
		SetVertexShader("./shader/texture_overlay.vs");
		SetPixelShader("./shader/texture_overlay.ps");
	};
}
