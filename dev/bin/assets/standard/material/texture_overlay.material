
RenderState rs
{
	CULLMODE						= None;
	DepthEnable						= False;
	DepthFunc						= ALWAYS;
	DepthWriteMask					= ZERO;
	StencilEnable					= false;
	BlendEnable						= false;
};

Technique T1
{
	Pass P1
	{
		SetRenderState(rs);
		SetVertexShader("./shader/texture_overlay.vs");
		SetPixelShader("./shader/texture_overlay.ps");
	};
}
