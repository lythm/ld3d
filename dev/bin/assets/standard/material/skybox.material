

RenderState rs
{
	FrontFace	= CW;
	CULLMODE	= none;
	FILLMODE	= SOLID;

	DepthEnable = TRUE;
	DepthFunc = LESS;
	DepthWriteMask = zero;
	StencilEnable = false;
	BlendEnable = false;

};

technique grid
{
  pass p1
  {
	SetRenderState(rs);
	SetVertexShader("./shader/skybox.vs");
	SetPixelShader("./shader/skybox.ps");
  }
}
